<html lang="{{ locale }}">
<head>
    <title>
        {{ page.title }}
    </title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    {% stylesheets '@DothivPremiumConfiguratorBundle/Resources/lesscss/styles.less' filter='cssrewrite' %}
    <link rel="stylesheet" href="{{ asset_url }}">
    {% endstylesheets %}
    {% image '@DothivCharityWebsiteBundle/Resources/public/images/favicon.png' %}
    <link rel="shortcut icon" href="{{ asset_url }}">
    {% endimage %}
    <link href='//fonts.googleapis.com/css?family=Source+Sans+Pro:300,600' rel='stylesheet' type='text/css'>
    {% javascripts
    '@DothivAngularJsBundle/Resources/angular/angular.js'
    filter='?ngmin,?closureecma5'
    output='js/*.min.js' %}
    <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}
    <base href="/">
</head>
<body data-ng-app="dotHIVApp">
<header data-ng-controller="HeaderController">
    <article>
        <section>
            <h1>
                {% image '@DothivPremiumConfiguratorBundle/Resources/public/images/dothiv-logo.png' %}
                <img src="{{ asset_url }}" alt="{{ page.title }}">
                {% endimage %}
                {{ page.title }}
            </h1>
            <div id="loading" data-ng-show="loading">
                {% trans %}pc.loading{% endtrans %}
                {% image '@DothivPremiumConfiguratorBundle/Resources/public/images/loader.gif' %}
                <img src="{{ asset_url }}" alt="{% trans %}pc.loading{% endtrans %}">
                {% endimage %}
            </div>
        </section>
    </article>
</header>
<main data-ui-view></main>
<footer></footer>
{% block angular_app %}
    {% javascripts
    '@DothivAngularJsBundle/Resources/angular/angular-resource.js'
    '@DothivAngularJsBundle/Resources/angular/angular-route.js'
    '@DothivAngularJsBundle/Resources/angular/angular-cookies.js'
    '@DothivAngularJsBundle/Resources/extra/angular-ui.github.io/ui-router/angular-ui-router.js'
    '@DothivPremiumConfiguratorBundle/Resources/public/js/app.js'
    '@DothivBaseWebsiteBundle/Resources/public/1.2/services/*.js'
    '@DothivBaseWebsiteBundle/Resources/public/1.2/services/models/*.js'
    '@DothivPremiumConfiguratorBundle/Resources/public/js/services/*.js'
    '@DothivPremiumConfiguratorBundle/Resources/public/js/controllers/*.js'
    filter='?ngmin,?closureecma5'
    output='js/*.min.js' %}
    <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}
{% endblock angular_app %}
<script type="text/javascript">
    'use strict';
    angular.module('dotHIVApp.services').factory('config', function () {
        var config = {
            locale: '{{ locale }}',
            domain: '{{ app.request.get('domain') }}'
        };
        return config;
    });
</script>
</body>
</html>
