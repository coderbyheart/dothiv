{% import "DothivBaseWebsiteBundle:Macros:cf.html.twig" as cf %}
{% for block in page.blocks %}
    {% if block.behaviour|default('block') == "quotes_list" %}
        {% include "DothivCharityWebsiteBundle:behaviour:quotes_list.html.twig" with {'block': block, 'level': 2} %}
    {% else %}
        <div class="row-fluid">
            {{ cf.block(block, 2) }}
        </div>
    {% endif %}
{% endfor %}
